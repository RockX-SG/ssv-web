{
  "name": "ssv-web",
  "description": "Webapp to interact with SSV",
  "version": "1.0.0",
  "dependencies": {
    "@chainsafe/bls": "^6.0.1",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "bls-signatures": "^0.2.5",
    "bnc-onboard": "^1.23.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "ethereumjs-wallet": "^1.0.1",
    "jest-html-reporter": "^3.3.0",
    "link-module-alias": "^1.2.0",
    "material-ui-dropzone": "^3.5.0",
    "mobx": "5.15.0",
    "mobx-react": "6.1.4",
    "react": "^17.0.2",
    "react-app-env": "^1.2.3",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "^1.32.11",
    "scrypt-js": "^3.0.1",
    "shamirs-secret-sharing-ts": "^1.0.2",
    "styled-components": "^5.2.3",
    "typescript": "~4.1.5",
    "web3": "^1.3.5"
  },
  "scripts": {
    "start": "react-app-env --env-file=.env.local start",
    "build": "react-app-env --env-file=.env.${BUILD_ENV} build",
    "test": "cross-env NODE_ENV=test TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' DOTENV=.env.staging node -r dotenv/config ./node_modules/.bin/mocha -r ts-node/register -r jsdom-global/register './tests/*.test.*'",
    "test:dev": "react-scripts test --coverage --reporters=default",
    "lint": "eslint 'src/**'",
    "eject": "react-scripts eject",
    "postinstall": "link-module-alias && husky install",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "license": "GPL-3.0",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "_moduleAliases": {
    "~root": "./src",
    "~app": "./src/app",
    "~lib": "./src/lib"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/register": "^7.13.16",
    "@types/chai": "^4.2.17",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-preset-react-app-babel-7": "^4.0.2-0",
    "chai": "^4.3.4",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "husky": "^6.0.0",
    "ignore-styles": "^5.0.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^8.3.2",
    "pinst": "^2.1.6",
    "sass-lint": "^1.13.1",
    "ts-node": "^9.1.1"
  }
}
