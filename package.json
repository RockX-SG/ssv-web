    "bls-signatures": "^0.2.5",
    "bnc-onboard": "^1.23.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "ethereumjs-wallet": "^1.0.1",
    "link-module-alias": "^1.2.0",
    "material-ui-dropzone": "^3.5.0",
    "mobx": "5.15.0",
    "mobx-react": "6.1.4",
    "react": "^17.0.2",
    "react-app-env": "^1.2.3",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sass": "^1.32.11",
    "scrypt-js": "^3.0.1",
    "shamirs-secret-sharing-ts": "^1.0.2",
    "styled-components": "^5.2.3",
    "typescript": "~4.1.5",
    "web3": "^1.3.5"
  },
  "scripts": {
    "start": "react-app-env --env-file=.env.local start",
    "build": "react-app-env --env-file=.env.${BUILD_ENV} build",
    "test": "cross-env NODE_ENV=test DOTENV=.env.staging node -r dotenv/config ./node_modules/.bin/react-scripts test --watchAll=false",
    "test:dev": "react-scripts test --coverage --reporters=default",
    "lint": "eslint 'src/**'",
    "eject": "react-scripts eject",
    "postinstall": "link-module-alias && husky install",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "license": "GPL-3.0",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "_moduleAliases": {
    "~root": "./src",
    "~app": "./src/app",
    "~lib": "./src/lib"
  },
  "jest": {},
  "devDependencies": {
    "@testing-library/dom": "^7.30.4",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^24.0.0",
    "@types/jsdom": "^16.2.10",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "husky": "^6.0.0",
    "jest-html-reporter": "^3.3.0",
    "jsdom-global": "^3.0.2",
    "pinst": "^2.1.6",
    "sass-lint": "^1.13.1",
    "ts-node": "^9.1.1"
  }
