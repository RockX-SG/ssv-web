{
  "name": "ssv-web",
  "description": "Webapp to interact with SSV",
  "version": "1.0.0",
  "_moduleAliases": {
    "~root": "./src",
    "~app": "./src/app",
    "~lib": "./src/lib",
    "~config": "./config"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "resetMocks": true,
    "testEnvironment": "~config/jest/TestsEnvironments",
    "testTimeout": 10000
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
